{
  "name": "predicty-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:report": "vitest --environment jsdom --root src/ --reporter=json --outputFile=../.storybook/.jest-test-results.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test-preview": "yarn build && npx http-server dist",
    "prepare": "cd .. && husky install pwa/.husky",
    "storybook": "SET NODE_ENV=test & start-storybook -p 6006 --disable-telemetry",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@types/facebook-js-sdk": "^3.3.6",
    "@types/gapi": "^0.0.44",
    "pinia": "^2.0.28",
    "register-service-worker": "^1.7.2",
    "remove": "^0.1.5",
    "sass": "^1.58.3",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@etchteam/storybook-addon-status": "^4.2.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-jest": "^6.5.16",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "0.1.39",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.9.3",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.9",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/components": "^9.12.0",
    "@vueuse/core": "^9.12.0",
    "autoprefixer": "^10.4.13",
    "element-plus": "^2.2.30",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-vue": "^9.3.0",
    "fetch-inject": "^3.2.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "maska": "^2.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.6",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.0.0",
    "vite-plugin-mkcert": "^1.13.3",
    "vite-plugin-pwa": "^0.14.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12",
    "workbox-precaching": "^6.5.4"
  }
}
